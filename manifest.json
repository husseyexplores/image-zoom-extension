{
  "name": "CTRL+Enter Image Zoom",
  "version": "0.2.5",
  "author": "Husseyexplores",
  "manifest_version": 2,
  "description": "Zoom image with CTRL+Enter",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end",
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}